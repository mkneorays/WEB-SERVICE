package com.itcs.beans;
public class Person {
	 
    
    private String name;
    private String email;
    private String username;
    private String password;
    private int age;
    private String address;
    private String Occupation;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public String getOccupation() {
		return Occupation;
	}
	public void setOccupation(String occupation) {
		Occupation = occupation;
	}
	@Override
	public String toString() {
		return "Person [name=" + name + ", email=" + email + ", username=" + username + ", password=" + password
				+ ", age=" + age + ", address=" + address + ", Occupation=" + Occupation + "]";
	}
	
	
}
----------------------------------------------------------------------------------------------------------------
 package com.itcs.beans;

 public class UserData {
	  private String nickName;
	    private String semail;
	    private long mobile;
	    private String job;
		public String getNickName() {
			return nickName;
		}
		public void setNickName(String nickName) {
			this.nickName = nickName;
		}
		public String getSemail() {
			return semail;
		}
		public void setSemail(String semail) {
			this.semail = semail;
		}
		public long getMobile() {
			return mobile;
		}
		public void setMobile(long mobile) {
			this.mobile = mobile;
		}
		public String getJob() {
			return job;
		}
		public void setJob(String job) {
			this.job = job;
		}
		@Override
		public String toString() {
			return "UserData [nickName=" + nickName + ", semail=" + semail + ", mobile=" + mobile + ", job=" + job
					+ "]";
		}
	    
}
------------------------------------------------------------------------------------------------------------------
package com.itcs.controller;

import javax.servlet.http.HttpSession;

import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.ModelAttribute;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestParam;

import org.springframework.web.bind.annotation.SessionAttributes;

import com.itcs.beans.Person;
import com.itcs.beans.UserData;

@Controller
@SessionAttributes(names = { "person", "userdata" })
public class LoginController {

	@ModelAttribute("person")
	public Person sessionPage() {
		System.out.println("sessionPage method");
		return new Person();

	}// method
	@ModelAttribute("userdata")
	public UserData session1Page() {
		System.out.println("sessionPage method");
		return new UserData();

	}// method

	@RequestMapping("/home.htm")
	public String homePage() {
		System.out.println("home method");

		return "home";

	}// method

	@RequestMapping("/login.htm")
	public String login(@RequestParam("user") String user, @RequestParam String pwd) {
		System.out.println("login method  " + user + "  " + pwd);

		return "form1";

	}// method

	@RequestMapping("/form1.htm")
	public String form1Method(Model map, @ModelAttribute("person") Person person) {
		System.out.println("form1Method method  " + person);
		
		map.addAttribute("form1", person);
		return "form2";

	}// method

	@RequestMapping("/form2.htm")
	public String form2Method(Model map, @ModelAttribute("person") Person person) {
		System.out.println("form2Method method  " + person);
		map.addAttribute("form2", person);
		return "form3";

	}// method

	@RequestMapping("/form3.htm")
	public String form3Method( Model map, @ModelAttribute("userdata") UserData userdata,HttpSession ses ) {
		Person p=(Person) ses.getAttribute("person");
		System.out.println("form3Method method  " + userdata+"   =="+p);
		
		map.addAttribute("form3", userdata);
		map.addAttribute("form", p);
		return "form4";

	}// method
}// class
